<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/container"
android:layout_width="match_parent"
android:layout_height="wrap_content"
android:orientation="vertical"
android:paddingBottom="16dp"
android:paddingLeft="16dp"
android:paddingRight="16dp"
android:paddingTop="16dp"
tools:context="com.nisha.googleflights.Search_Flights"
android:weightSum="2">
<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">


    <EditText
        android:id="@+id/rtn_dte"
        android:layout_width="120dp"
        android:layout_height="76dp"
        android:textColor="@android:color/darker_gray"
        android:paddingTop="@dimen/nav_header_vertical_spacing">

    </EditText>
</LinearLayout>




<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">


</LinearLayout>
<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">


    <Button
        android:id="@+id/btn_rtnc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="ClearReturn"
        android:text=" Clear Return" />
    <Button
        android:id="@+id/btn_rtns"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="SaveReturn"
        android:text="Save Return" />

</LinearLayout>
</LinearLayout>

