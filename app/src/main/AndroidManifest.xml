<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nisha.googleflights">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FlightBooking"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Search_Flights">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Leaving_from" />
        <activity android:name=".Going_to" />
        <activity android:name=".Departure_date" />
        <activity android:name=".Return_date" />
        <activity android:name=".Passenger" />
        <activity android:name=".Passenger_class" />
        <activity android:name=".New_class" />
        <activity android:name=".Swipe" />
        <activity android:name=".Register"></activity>
    </application>

</manifest>